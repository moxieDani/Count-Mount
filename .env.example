# SvelteKit environment variables must be prefixed with VITE_ to be exposed to the client
# See: https://kit.svelte.dev/docs/modules#$env-static-public

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Google API Key (for client-side API calls)
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Auth.js Secret (generate random string) - Used for server-side auth, which is now disabled
AUTH_SECRET=your_auth_secret_here

# Trusted Host (for production) - Used for server-side auth, which is now disabled
AUTH_TRUST_HOST=true
